@name: '[address]';  
@sans: 'Open Sans Regular';
#a {
  text-name: @name;
  text-face-name: @sans;
  text-halo-radius: 0.2;
  text-size: 16;
  text-character-spacing: 0.5;
   text-dx: 15;
  text-fill:#d8fb00;
}
#a {
  marker-width:20;
  marker-fill: rgba(18, 53, 192, 0.5);
  marker-allow-overlap:true;
}

