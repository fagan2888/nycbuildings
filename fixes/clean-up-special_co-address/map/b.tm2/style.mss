@name: '[address]';  
@sans: 'Open Sans Regular';
#b {
  text-name: @name;
  text-face-name: @sans;
  text-halo-radius: 0.2;
  text-size: 16;
  text-character-spacing: 0.5;
   text-dx: 15;
  text-fill:#d8fb00;
}
#b {
  marker-width:20;
  marker-fill: rgba(255, 0, 41, 0.5);
  marker-allow-overlap:true;
}
